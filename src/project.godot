; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Gladius"
run/main_scene="res://scenes/game_states/Main.tscn"
config/icon="res://assets/flagtemp.png"

[autoload]

CONFIG="*res://scripts/global/gb_config.gd"
STAT="*res://scripts/global/gb_attributes_table.gd"
utils="*res://scripts/global/gb_utils.gd"
game="*res://scripts/global/gb_game.gd"
gb_DataParser="*res://scripts/global/gb_data_parser.gd"
gb_ItemDatabase="*res://scripts/global/gb_item_database.gd"
gb_Z_Index_Orderer="*res://scripts/global/gb_z_index_orderer.gd"
gb_CombatText="*res://scripts/global/gb_combat_text.gd"

[debug]

gdscript/warnings/unassigned_variable=false
gdscript/warnings/unassigned_variable_op_assign=false
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_class_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/standalone_expression=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false
gdscript/warnings/unsafe_cast=true

[display]

window/size/width=1366
window/size/height=768
window/size/test_width=1366
window/size/test_height=768
window/allow_per_pixel_transparency=true
mouse_cursor/custom_image="res://assets/crosshair149.png"
window/stretch/aspect="keep"
window/stretch/shrink="1"

[editor_plugins]

enabled=PoolStringArray( "sprite_quick_offset" )

[importer_defaults]

texture={
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}
ogg_vorbis={
"loop": false,
"loop_offset": 0
}

